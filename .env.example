# WooCommerce Backend
NEXT_PUBLIC_WORDPRESS_URL=http://your-wordpress-site.local
NEXT_PUBLIC_GRAPHQL_ENDPOINT=http://your-wordpress-site.local/graphql

# WooCommerce REST API
WOOCOMMERCE_URL=http://your-wordpress-site.local
WOOCOMMERCE_CONSUMER_KEY=your_consumer_key_here
WOOCOMMERCE_CONSUMER_SECRET=your_consumer_secret_here

# JWT Authentication
JWT_SECRET_KEY=your_jwt_secret_key_here
NEXT_PUBLIC_JWT_AUTH_ENDPOINT=http://your-wordpress-site.local/wp-json/jwt-auth/v1

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
